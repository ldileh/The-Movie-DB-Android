<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ContainerView">
        <attr name="show_view" format="enum">
            <enum name="content" value="1"/>
            <enum name="error" value="2"/>
            <enum name="loading" value="3"/>
            <enum name="empty" value="4"/>
        </attr>
        <attr name="error_message" format="string|reference"/>
        <attr name="empty_message" format="string|reference"/>
        <attr name="content_view_id" format="reference|integer"/>
    </declare-styleable>
</resources>